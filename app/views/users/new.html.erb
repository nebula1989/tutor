<div id="clearance" class="sign-up center_div dark-background">
  <h2><%= t(".title") %></h2>

  <%= bootstrap_form_for(@user) do |form| %>
    <%= render partial: '/users/form', object: form %>

    <div class="submit-field">
      <%= form.submit %>
    </div>

    <div class="other-links">
      <%= link_to t(".sign_in"), sign_in_path %>
    </div>
  <% end %>
</div>
