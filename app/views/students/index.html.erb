<p id="notice"><%= notice %></p>

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
       <td>Name</td>
       <td>Age</td>
       <td>Level</td>
       <td>Location</td>
       <td>Birthday</td>
       <td>Strength_Weak</td>
       <td>Teacher_ID</td>
    </tr>
    <% @student.each do |student| %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.age %></td>
        <td><%= student.level %></td>
        <td><%= student.location %></td>
        <td><%= student.birthday %></td>
        <td><%= student.strength_weak %></td>
        <td><%= student.user_id %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @student %>

<br>

<%= link_to 'New Student', new_student_path %><br>
<%= link_to 'Back to Dashboard', dashboard_path %>
